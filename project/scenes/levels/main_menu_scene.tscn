[sub_resource type="GDScript" id="GDScript_ktd6h"]
script/source = "extends Node2D

var elapsed_time = 0
var showing = false

func _ready():
	visible = false

func _process(delta: float) -> void:
	if !showing:
		elapsed_time += delta
		if elapsed_time > 10:
			showing = true
			visible = true
			$Sprite2D.start_anim()
	
	
"

