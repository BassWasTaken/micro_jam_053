script/source = "extends Sprite2D

var tween

func _ready():
	modulate = Color(1,1,1,0)

func start_anim() -> void:
	tween = create_tween()
	tween.set_loops()
	tween.tween_property(self, \"modulate\", Color(1,1,1,0.5), 0.5)
	tween.tween_property(self, \"modulate\", Color(1,1,1,0), 0.5)
"

